{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "ColtApi": {
    "BaseUrl": "https://api.colt.net/ebonding/v5",
    "TokenUrl": "https://api.colt.net/auth/token",
    "ClientId": "",
    "ClientSecret": "",
    "Timeout": 30,
    "RetryCount": 3,
    "CircuitBreakerThreshold": 5
  },
  "Authentication": {
    "Jwt": {
      "Issuer": "ColteBondingAPI",
      "Audience": "ColteBondingAPI",
      "SecretKey": "",
      "ExpirationMinutes": 60
    }
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "Window": "00:01:00",
    "QueueProcessingOrder": "OldestFirst",
    "QueueLimit": 50
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "ColteBondingAPI"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ColteBondingDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}